<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>controlAltaVotante</servlet-name>
        <servlet-class>controlador.controlAltaVotante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controlListadoVotantes</servlet-name>
        <servlet-class>controlador.controlListadoVotantes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controlIdentificacion</servlet-name>
        <servlet-class>controlador.controlIdentificacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controlVotar</servlet-name>
        <servlet-class>controlador.controlVotar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controlBajaVotante</servlet-name>
        <servlet-class>controlador.controlBajaVotante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controlEscrutinio</servlet-name>
        <servlet-class>controlador.controlEscrutinio</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>controlAltaVotante</servlet-name>
        <url-pattern>/controlAltaVotante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlListadoVotantes</servlet-name>
        <url-pattern>/controlListadoVotantes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlIdentificacion</servlet-name>
        <url-pattern>/controlIdentificacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlVotar</servlet-name>
        <url-pattern>/controlVotar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlBajaVotante</servlet-name>
        <url-pattern>/controlBajaVotante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlEscrutinio</servlet-name>
        <url-pattern>/controlEscrutinio</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
